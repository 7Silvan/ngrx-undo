{
  "name": "ngrx-undo",
  "version": "0.0.5",
  "description": "Undo support for @ngrx/store",
  "scripts": {
    "karma": "karma start --single-run",
    "test": "npm run karma",
    "clean:pre": "rimraf release",
    "clean:post": "rimraf src/**/*.ngfactory.ts",
    "copy": "cpy LICENSE package.json README.md release",
    "build:js": "ngc -p tsconfig.dist.json",
    "build:umd": "rollup -c rollup.config.js",
    "build:uglify": "uglifyjs -c --screw-ie8 --comments -o ./release/bundles/store-devtools.min.umd.js ./release/bundles/store-devtools.umd.js",
    "prebuild": "npm run test && npm run clean:pre",
    "postbuild": "npm run clean:post && npm run copy",
    "build": "npm run build:js && npm run build:umd && npm run build:uglify",
    "prepublish": "npm run build"
  },
  "bugs": {
    "url": "https://github.com/brechtbilliet/ngrx-undo/issues"
  },
  "homepage": "https://github.com/brechtbilliet/ngrx-undo#readme",
  "main": "./bundles/store-devtools.umd.js",
  "module": "./index.js",
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">= 4.2.1 <= 6",
    "npm": ">= 3"
  },
  "devDependencies": {
    "@angular/compiler": "2.0.0",
    "@angular/compiler-cli": "0.6.3",
    "rimraf": "2.5.4",
    "@angular/core": "2.0.0",
    "@ngrx/core": "1.2.0",
    "@ngrx/store": "2.2.1",
    "rxjs": "5.0.0-beta.12",
    "@types/jasmine": "^2.2.33",
    "@types/node": "^6.0.38",
    "awesome-typescript-loader": "^2.2.1",
    "core-js": "^2.4.1",
    "cpy-cli": "^1.0.1",
    "istanbul-instrumenter-loader": "^0.2.0",
    "jasmine": "^2.5.0",
    "karma": "^1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.0.2",
    "karma-mocha-reporter": "^2.1.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "rollup": "^0.34.13",
    "ts-loader": "^0.8.2",
    "tslint": "^3.15.1",
    "tslint-loader": "^2.1.5",
    "typescript": "^2.0.2",
    "uglifyjs": "^2.4.10",
    "webpack": "^2.1.0-beta.21",
    "zone.js": "^0.6.17"
  },
  "peerDependencies": {
    "rxjs": "^5.0.0-beta.12",
    "@ngrx/store": "^2.0.0"
  }
}
